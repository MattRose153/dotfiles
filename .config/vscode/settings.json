{
    "terminal.integrated.inheritEnv": false,
    "kite.showWelcomeNotificationOnStartup": false,
    "autoDocstring.customTemplatePath": "/Users/matt/.config/vscode/.mustache",
    "autoDocstring.startOnNewLine": true,
    "explorer.confirmDragAndDrop": false,
    "editor.minimap.enabled": false,
    "go.formatTool": "goimports",
    "editor.wordWrap": "on",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path.mac": "/Users/matt/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.helm-path.mac": "/Users/matt/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.draft-path.mac": "/Users/matt/.vs-kubernetes/tools/draft/darwin-amd64/draft"
    },
    "window.zoomLevel": 0,
    "editor.rulers": [
        88
    ],
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "basic",
    "terraform.languageServer": {
        "external": true,
        "pathToBinary": "",
        "args": [
            "serve"
        ],
        "maxNumberOfProblems": 100,
        "trace.server": "off"
    },
    "[terraform]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 2
    },
    "jupyter.sendSelectionToInteractiveWindow": false,
    "jupyter.askForKernelRestart": false,
    "launch": {
        "configurations": [],
        "compounds": []
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.editorAssociations": [],
    "go.enableCodeLens": {
        "references": false,
        "runtest": true
    },
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Monokai Extended",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": "meta.function-call.generic.python",
            "settings": {
              "foreground": "#66D9EF"
            }
          }
        ]
      }
}
